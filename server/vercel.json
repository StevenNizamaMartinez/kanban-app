{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "allow": ["https://tu-sitio-web.com", "https://otro-sitio-web.com"],
      "headers": ["Authorization", "Content-Type"]
    }
  ]
}
